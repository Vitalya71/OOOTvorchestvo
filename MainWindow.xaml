<Window x:Class="OOOTvorchestvo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OOOTvorchestvo"
        mc:Ignorable="d"
        Title="ООО «Творчество»" Height="450" Width="800" Icon="Images\Icon\icon.ico" WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="34*"/>
            <RowDefinition Height="183*"/>
        </Grid.RowDefinitions>
        <TextBlock Text="ООО «Творчество»" Style="{StaticResource ResourceKey=TBStyle}" Foreground="{StaticResource AccentColor}" HorizontalAlignment="Center" VerticalAlignment="Center"></TextBlock>
        <Frame x:Name="MainFrame" Grid.RowSpan="2" Grid.Row="1" ScrollViewer.HorizontalScrollBarVisibility="Disabled"></Frame>
        <Image Source="Images\logo.png" HorizontalAlignment="Left"></Image>
    </Grid>
</Window>
